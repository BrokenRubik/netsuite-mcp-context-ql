{
    "parser": "espree",
    "ecmaFeatures": {},
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "require": false,
        "define": false,
        "jQuery": false,
        "Backbone": false,
        "_": false,
        "moment": false,
        "SC": false,
        "request": false,
        "response": false,
        "nlapiGetCache": false,
        "nlapiGetContext": false,
        "nlapiLogExecution": false,
        "nlobjSearchFilter": false,
        "nlobjSearchColumn": false,
        "nlapiSearchRecord": false,
        "nlapiSubmitField": false,
        "nlapiLookupField": false,
        "nlapiGetRecordType": false,
        "nlapiGetRecordId": false,
        "nlapiGetFieldValue": false,
        "nlapiCreateRecord": false,
        "nlapiStringToDate": false,
        "nlapiDateToString": false,
        "notFoundError": false,
        "nlapiGetUser": false,
        "nlapiSubmitRecord": false,
        "nlapiCreateError": false,
        "nlobjError": false,
        "nlapiResolveURL": false,
        "nlapiRequestURL": false,
        "nlapiSetFieldValue": false,
        "nlapiCreateSearch": false,
        "nlapiCreateForm": false,
        "nlapiSubmitFile": false,
        "nlapiLoadFile": false,
        "nlapiAttachRecord": false,
        "nlapiDeleteFile": false,
        "nlapiScheduleScript": false,
        "nlapiSendEmail": false,
        "nlapiLoadRecord": false,
        "nlapiGetLogin": false,
        "nlapiDeleteRecord": false,
        "nlapiYieldScript": false,
        "nlapiCopyRecord": false,
        "nlapiLoadSearch": false,
        "nlapiGetLineItemCount": false,
        "nlapiGetLineItemValue": false,
        "nlapiGetLineItemText": false,
        "nlapiSetLineItemValue": false,
        "nlapiXMLToPDF": false,
        "nlapiCreateFile": false,
        "Application": false,
        "nlapiGetJobManager": false,
        "log": false,
        "session": false,
        "customer": false,
        "methodNotAllowedError": false,
        "unauthorizedError": false,
        "nlapiGetOldRecord": false,
        "nlapiSetRedirectURL": false,
        "nlapiGetNewRecord": false,
        "nlapiEncrypt": false,
        "SearchHelper": false,
        "nlapiSetCurrentLineItemValue": false,
        "nlapiSetFieldText": false,
        "nlapiGetCurrentLineItemValue": false,
        "nlapiCreateTemplateRenderer": false,
        "nlapiAddDays": false,
        "nlapiGetField": false
    },
    "rules": {
        "no-extra-parens": 1,
        "curly": 1,
        "eqeqeq": 1,
        "no-else-return": 1,
        "no-eval": 1,
        "no-implicit-coercion": 1,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 1,
        "no-native-reassign": [
            2,
            {
                "exceptions": [
                    "Object"
                ]
            }
        ],
        "no-new-wrappers": 1,
        "no-new": 2,
        "no-param-reassign": 2,
        "no-redeclare": 2,
        "no-return-assign": 1,
        "no-self-compare": 2,
        "vars-on-top": 1,
        "no-shadow-restricted-names": 2,
        "no-shadow": 1,
        "no-undef-init": 1,
        "no-undefined": 2,
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "camelcase": 0,
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "func-names": 0,
        "indent": 0,
        "new-cap": 0,
        "new-parens": 2,
        "no-array-constructor": 1,
        "no-continue": 1,
        "no-inline-comments": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-unneeded-ternary": 2,
        "semi": [
            2,
            "always"
        ],
        "space-before-blocks": 1,
        "space-before-function-paren": [
            0,
            {
                "anonymous": "never",
                "named": "never"
            }
        ],
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ]
    }
}